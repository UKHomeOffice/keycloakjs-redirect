{
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-alert": 2,
    "no-div-regex": 2,
    "no-dupe-class-members": 2,
    "no-else-return": 2,
    "no-empty-label": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-parens": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-lone-blocks": 1,
    "no-lonely-if": 2,
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "no-native-reassign": 2,
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 1,
    "no-param-reassign": 0,
    "no-path-concat": 2,
    "no-process-env": 2,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sync": 2,
    "no-trailing-spaces": 2,
    "no-this-before-super": 2,
    "no-undef-init": 2,
    "no-unexpected-multiline": 2,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-magic-numbers": 2,
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "arrow-parens": 0,
    "arrow-spacing": 2,
    "block-scoped-var": 1,
    "brace-style": 2,
    "callback-return": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "complexity": [
      1,
      11
    ],
    "consistent-return": 1,
    "consistent-this": [
      1,
      "that"
    ],
    "constructor-super": 2,
    "curly": [
      2,
      "all"
    ],
    "default-case": 1,
    "dot-location": [
      2,
      "property"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eol-last": 2,
    "eqeqeq": 2,
    "func-style": [
      2,
      "expression",
      {
        "allowArrowFunctions": true
      }
    ],
    "global-require": 2,
    "handle-callback-err": 2,
    "id-length": [
      1,
      {
        "min": 3,
        "max": 15,
        "properties": "never",
        "exceptions": [
          "_"
        ]
      }
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "max-depth": [
      1,
      4
    ],
    "max-len": [
      1,
      100,
      4
    ],
    "max-params": [
      1,
      3
    ],
    "max-statements": [
      1,
      10
    ],
    "new-cap": 1,
    "new-parens": 2,
    "one-var": [
      2,
      "never"
    ],
    "operator-linebreak": [
      2,
      "before",
      {
        "overrides": {
          "?": "after"
        }
      }
    ],
    "padded-blocks": [
      2,
      "never"
    ],
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-reflect": 1,
    "prefer-template": 1,
    "quote-props": [
      2,
      "as-needed"
    ],
    "quotes": [
      0,
      "double"
    ],
    "semi": 2,
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "sort-vars": 2,
    "space-after-keywords": [
      2,
      "always"
    ],
    "space-before-keywords": [
      2,
      "always"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "always"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-return-throw-case": 2,
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "spaced-comment": 2,
    "vars-on-top": 1,
    "wrap-iife": 2,
    "wrap-regex": 1,
    "yoda": [
      2,
      "never"
    ]
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": false
  },
  "globals": {
    "reverseRouteService": true,
    "Centres": true,
    "RequestValidatorService": true,
    "IrcEntryHeartbeatValidatorService": true,
    "expect": true,
    "_": true,
    "request": true,
    "sinon": true,
    "sails": true,
    "Sails": true
  },
  "extends": "eslint:recommended"
}
